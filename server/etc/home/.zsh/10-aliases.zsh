# Shortened
alias bunr="bun repl"
alias fdi="fd -i"
alias j=just
alias rgi="rg -i"
alias rgf='rg --files-with-matches'
alias wgetc="wget --content-disposition"
alias wgeto="wget -O"

# Process management
alias pg="ps aux | grep"
alias psef="ps -ef"
alias ps-latest="ps -ef --sort=start_time"

# Git
alias g="git"
alias ga="git add"
alias gc="git commit"
alias gs="git status"
alias gd="git diff"
alias gf="git fetch"
alias gm="git merge"
alias gr="git rebase"
alias gp="git push"
alias gu="git unstage"
alias gg="git graph"
alias ggg="git graphgpg"
alias gco="git checkout"
alias gcs="git commit -S"

# Quickly traverse back up file tree
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias ......='cd ../../../../..'
alias .......='cd ../../../../../..'

